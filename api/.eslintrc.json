{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "indent": ["error", 2],
    "max-len": ["error", { "code": 120 }],
    "no-console": "warn",
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off",
    "linebreak-style": "off",
    "consistent-return": "off",
    "import/prefer-default-export": "off",
    "arrow-body-style": "off"
  }
}
